PROJECT_NAME="SGE Dashboard"
VERSION="0.1.0"
DEBUG=true
ENV="development"

# Security
SECRET_KEY="development-secret-key-please-change-in-production"
JWT_SECRET_KEY="development-jwt-key-please-change-in-production"
